{
  "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "config-overrides.js",
      "src/**/*.json",
      "node_modules/**/*.ts"
    ]
  }
}

